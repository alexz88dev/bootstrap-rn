{
  "expo": {
    "name": "MyCar Portrait",
    "slug": "mycar-portrait",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mycarportrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "fingerprint"
    },
    "updates": {
      "url": "https://u.expo.dev/8385d647-791b-4998-b66e-c6b11e4939a0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alexzamfir.mycarportrait",
      "buildNumber": "1",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app uses the camera to capture photos of your car for creating custom portraits and widgets."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.alexzamfir.mycarportrait",
      "versionCode": 1
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-updates",
      "expo-dev-client",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "./plugins/withMyCarWidget.js",
      [
        "@bittingz/expo-widgets",
        {
          "ios": {
            "devTeamId": "YOUR_TEAM_ID",
            "appGroups": ["group.com.alexzamfir.mycarportrait"]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8385d647-791b-4998-b66e-c6b11e4939a0"
      }
    },
    "owner": "anaisdigitaldemo"
  }
}
