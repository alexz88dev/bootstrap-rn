{
  "cli": {
    "version": ">= 16.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "cache": {
        "key": "development-cache"
      },
      "ios": {
        "resourceClass": "m-medium",
        "simulator": true,
        "buildConfiguration": "Debug",
        "autoIncrement": true
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "autoIncrement": true
      },
      "channel": "development",
      "env": {
        "APP_ENV": "development",
        "EAS_BUILD_ENVIRONMENT": "development"
      }
    },
    "staging": {
      "developmentClient": false,
      "distribution": "internal",
      "cache": {
        "key": "staging-cache"
      },
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release",
        "autoIncrement": true
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "autoIncrement": true
      },
      "channel": "staging",
      "env": {
        "APP_ENV": "staging",
        "EAS_BUILD_ENVIRONMENT": "staging"
      }
    },
    "production": {
      "cache": {
        "key": "production-cache"
      },
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": true
      },
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": true
      },
      "channel": "production",
      "env": {
        "APP_ENV": "production",
        "EAS_BUILD_ENVIRONMENT": "production"
      }
    }
  }
}
