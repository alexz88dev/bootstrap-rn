{
  "cli": {
    "version": ">= 16.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "cache": {
        "key": "mycar-development-cache"
      },
      "ios": {
        "resourceClass": "m-medium",
        "simulator": true,
        "buildConfiguration": "Debug",
        "autoIncrement": true,
        "bundleIdentifier": "com.alexzamfir.mycarportrait.dev"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "autoIncrement": true
      },
      "channel": "development",
      "env": {
        "APP_ENV": "development",
        "EAS_BUILD_ENVIRONMENT": "development",
        "EXPO_PUBLIC_APP_NAME": "MyCar Dev"
      }
    },
    "staging": {
      "developmentClient": false,
      "distribution": "internal",
      "cache": {
        "key": "mycar-staging-cache"
      },
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release",
        "autoIncrement": true,
        "bundleIdentifier": "com.alexzamfir.mycarportrait"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "autoIncrement": true
      },
      "channel": "staging",
      "env": {
        "APP_ENV": "staging",
        "EAS_BUILD_ENVIRONMENT": "staging",
        "EXPO_PUBLIC_APP_NAME": "MyCar Staging"
      }
    },
    "production": {
      "cache": {
        "key": "mycar-production-cache"
      },
      "ios": {
        "resourceClass": "m-medium",
        "autoIncrement": true,
        "bundleIdentifier": "com.alexzamfir.mycarportrait"
      },
      "android": {
        "buildType": "app-bundle",
        "autoIncrement": true,
        "package": "com.alexzamfir.mycarportrait"
      },
      "channel": "production",
      "env": {
        "APP_ENV": "production",
        "EAS_BUILD_ENVIRONMENT": "production",
        "EXPO_PUBLIC_APP_NAME": "MyCar Portrait"
      }
    }
  },
  "submit": {
    "staging": {
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "YOUR_ASC_APP_ID",
        "appleTeamId": "YOUR_TEAM_ID"
      },
      "android": {
        "serviceAccountKeyPath": "./credentials/google-play-service-account.json",
        "track": "internal"
      }
    },
    "production": {
      "ios": {
        "appleId": "your-apple-id@example.com",
        "ascAppId": "YOUR_ASC_APP_ID",
        "appleTeamId": "YOUR_TEAM_ID",
        "ascApiKeyPath": "./credentials/asc-api-key.json"
      },
      "android": {
        "serviceAccountKeyPath": "./credentials/google-play-service-account.json",
        "track": "production",
        "releaseStatus": "draft"
      }
    }
  }
}